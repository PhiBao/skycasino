# Frontend Environment Variables Example
# Copy this file to .env and fill in your values
# Vite requires variables to start with VITE_

# Sepolia Contract Address (deployed FHEBlackjack)
VITE_CONTRACT_ADDRESS="0x8a15d7Ed46AeF0D89519426903dFECC2729BA0e1"

# Sepolia addresses for other contracts
VITE_COINFLIP_CONTRACT_ADDRESS="0xC863E8103a518d248C838a2e273DcdBA1A1fB711"
VITE_POKER_CONTRACT_ADDRESS="0xa4C546e630bCA61736ECC72a2191E2a169d2835C"

# Infura API Key for RPC access
VITE_INFURA_API_KEY="your-infura-api-key-here"

# Network Configuration
VITE_CHAIN_ID="11155111"
VITE_CHAIN_NAME="Sepolia"

# WalletConnect Cloud Project ID (used by RainbowKit/Wagmi)
# Get one at https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID="5c6c49dff8832b0677349a00036b646e"
